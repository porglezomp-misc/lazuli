(struct point x y)

(defn (point/length pt)
  (let! x (point/x pt))
  (let! y (point/y pt))
  (return (+ (* x x) (* y y))))

(defn (fib-helper a b n)
  (if (<= n 0)
    (return a))
  (if (== n 1)
    (return b))
  (fib-helper b (+ a b) (- n 1)))

(defn (fib n)
  (fib-helper 0 1 n))

(let! x 2.0)
(let! y 3.0)
(let! a-point (point x y))
(print a-point (point/x a-point) (point/y a-point))
(print (fib 10))
